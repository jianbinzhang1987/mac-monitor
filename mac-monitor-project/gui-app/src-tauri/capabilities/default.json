{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for the Mac Monitor GUI",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:default",
    "autostart:default",
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:window:allow-maximize",
    "core:window:allow-center",
    "core:window:allow-set-size",
    "core:app:default",
    "core:resources:default",
    "core:image:default",
    "core:menu:default",
    "core:tray:default"
  ]
}